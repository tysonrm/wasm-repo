interface port {
  variant port-error {
    error(string),
  }

  record port-data {
    data: list<u8>,
    wfid: string,
    wfstate: string,
    wfhist: list<string>
  }

  resource ports {
    constructor();
    invoke-port: func(name: string, data: port-data) -> result<port-data, port-error>;
  }
}